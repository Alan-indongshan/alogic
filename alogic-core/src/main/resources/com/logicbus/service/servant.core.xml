<?xml version="1.0" encoding="UTF-8"?>
<catalog name="root">
	<catalog name="core">
		<service 
			id = "ServiceDetailQuery" 
			name="ServiceDetailQuery" 
			note="查询指定的服务信息" 
			visible="system"
			log = "none" 
			module="com.logicbus.service.ServiceDetailQuery">
			<properties>
				<parameter id="jsonDefault" value="true"/>
				<parameter id="servant.maxActive" value="10"/>
				<parameter id="servant.maxIdle" value="1"/>
			</properties>
		</service>		
		<service 
			id="ServiceReload" 
			module="com.logicbus.service.ServiceReload" 
			name="ServiceReload" 
			note="重新装入服务缓冲池"
			log = "brief"
			visible="system"
		>
			<properties>
				<parameter id="jsonDefault" value="true"/>
				<parameter id="servant.maxActive" value="10"/>
				<parameter id="servant.maxIdle" value="1"/>
			</properties>			
		</service>	
		
		<service 
			id="ServicePause" 
			module="com.logicbus.service.ServicePause" 
			name="ServicePause" 
			note="暂停服务"
			log = "brief"
			visible="system"
		>
			<properties>
				<parameter id="jsonDefault" value="true"/>
				<parameter id="servant.maxActive" value="10"/>
				<parameter id="servant.maxIdle" value="1"/>
			</properties>			
		</service>	
		
		<service 
			id="ServiceResume" 
			module="com.logicbus.service.ServiceResume" 
			name="ServiceResume" 
			note="恢复服务"
			log = "brief"
			visible="system"
		>
			<properties>
				<parameter id="jsonDefault" value="true"/>
				<parameter id="servant.maxActive" value="10"/>
				<parameter id="servant.maxIdle" value="1"/>
			</properties>			
		</service>
		
		<service 
			id="ServiceQuery" 
			name="ServiceQuery" 
			note="查询系统中所部署的所有服务"
			visible="system"
			log = "none" 
			module="com.logicbus.service.ServiceQuery"
		>
			<properties>
				<parameter id="jsonDefault" value="true"/>
				<parameter id="servant.maxActive" value="10"/>
				<parameter id="servant.maxIdle" value="1"/>
			</properties>			
		</service>	
		<service 
			id="ServiceList" 
			name="ServiceList" 
			note="查询系统中所部署的所有服务列表(以列表形式输出)"
			visible="system"
			log = "none" 
			module="com.logicbus.service.ServiceList"
		>
			<properties>
				<parameter id="jsonDefault" value="true"/>
				<parameter id="servant.maxActive" value="10"/>
				<parameter id="servant.maxIdle" value="1"/>
			</properties>			
		</service>	
		
		<service 
			id="PooledServiceList" 
			name="PooledServiceList" 
			note="查询当前缓冲池中服务列表"
			visible="system"
			log = "none" 
			module="com.logicbus.service.PooledServiceList"
		>
			<properties>
				<parameter id="jsonDefault" value="true"/>
				<parameter id="servant.maxActive" value="10"/>
				<parameter id="servant.maxIdle" value="1"/>
			</properties>			
		</service>				
		<service 
			id="AclQuery" 
			name="AclQuery" 
			note="查询当前的访问控制列表"
			visible="system"
			log = "none" 
			module="com.logicbus.service.AclQuery"
		>
			<properties>
				<parameter id="jsonDefault" value="true"/>
				<parameter id="servant.maxActive" value="10"/>
				<parameter id="servant.maxIdle" value="1"/>
			</properties>			
		</service>
		
		<catalog name="util">
			<service 
				id="GC" 
				name="GC" 
				note="主动触发虚拟机回收内存" 
				visible="system"
				log = "brief" 
				module="com.logicbus.service.GC"
			>
				<properties>
					<parameter id="jsonDefault" value="true"/>
					<parameter id="servant.maxActive" value="10"/>
					<parameter id="servant.maxIdle" value="1"/>
				</properties>			
			</service>
					
			<service 
				id="Proxy" 
				name="Proxy" 
				note="代理服务"
				visible="system"
				log = "brief" 
				module="com.logicbus.service.Proxy"
			>
					<properties>
						<parameter id="proxy.path" value="/services/"/>
						<parameter id="proxy.forwarded" value="false"/>
						<parameter id="proxy.forwarded.header" value="${http.forwardedheader}"/>
						<parameter id="proxy.scheme" value="http"/>
						<parameter id="jsonDefault" value="true"/>
						<parameter id="servant.maxActive" value="10"/>
						<parameter id="servant.maxIdle" value="1"/>					
					</properties>
			</service>	
			
			<service 
				id="Settings" 
				name="Settings" 
				note="查询节点的环境变量"
				visible="system"
				log = "none" 
				module="com.logicbus.service.SettingsQuery"
			>
				<properties>
					<parameter id="jsonDefault" value="true"/>
					<parameter id="servant.maxActive" value="10"/>
					<parameter id="servant.maxIdle" value="1"/>
				</properties>		
			</service>	
			
			<service 
				id="Env" 
				name="Env" 
				note="查询JVM环境变量"
				visible="public"
				log = "none" 
				module="com.logicbus.service.EnvQuery"
			>
				<properties>
					<parameter id="jsonDefault" value="true"/>
					<parameter id="servant.maxActive" value="10"/>
					<parameter id="servant.maxIdle" value="1"/>
				</properties>		
			</service>					
		</catalog>
		
		<catalog name="log">	
			<service 
				id="Report" 
				name="Report" 
				note="业务日志报告"
				visible="system"
				log = "none" 
				module="com.logicbus.service.BizLogReport"
			>
				<properties>
					<parameter id="jsonDefault" value="true"/>
					<parameter id="servant.maxActive" value="10"/>
					<parameter id="servant.maxIdle" value="1"/>
				</properties>		
			</service>		
			<service 
				id="Pause" 
				name="Pause" 
				note="暂停BizLogger的处理"
				visible="system"
				log = "none" 
				module="com.logicbus.service.BizLoggerPause"
			>
				<properties>
					<parameter id="jsonDefault" value="true"/>
					<parameter id="servant.maxActive" value="10"/>
					<parameter id="servant.maxIdle" value="1"/>
				</properties>		
			</service>		
			<service 
				id="Resume" 
				name="Resume" 
				note="恢复BizLogger的处理"
				visible="system"
				log = "none" 
				module="com.logicbus.service.BizLoggerResume"
			>
				<properties>
					<parameter id="jsonDefault" value="true"/>
					<parameter id="servant.maxActive" value="10"/>
					<parameter id="servant.maxIdle" value="1"/>
				</properties>		
			</service>	
		</catalog>
		<catalog name="metrics">
			<service 
				id="Report" 
				name="Report" 
				note="指标处理报告"
				visible="system"
				log = "none" 
				module="com.logicbus.service.MetricsReport"
			>
				<properties>
					<parameter id="jsonDefault" value="true"/>
					<parameter id="servant.maxActive" value="10"/>
					<parameter id="servant.maxIdle" value="1"/>
				</properties>		
			</service>		
			<service 
				id="Pause" 
				name="Pause" 
				note="暂停MetricsHandler的处理"
				visible="system"
				log = "brief" 
				module="com.logicbus.service.MetricsPause"
			>
				<properties>
					<parameter id="jsonDefault" value="true"/>
					<parameter id="servant.maxActive" value="10"/>
					<parameter id="servant.maxIdle" value="1"/>
				</properties>		
			</service>		
			<service 
				id="Resume" 
				name="Resume" 
				note="恢复MetricsHandler的处理"
				visible="system"
				log = "brief" 
				module="com.logicbus.service.MetricsResume"
			>
				<properties>
					<parameter id="jsonDefault" value="true"/>
					<parameter id="servant.maxActive" value="10"/>
					<parameter id="servant.maxIdle" value="1"/>
				</properties>		
			</service>	
		</catalog>										
	</catalog>
</catalog>